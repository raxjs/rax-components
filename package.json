  },
  "scripts": {
    "setup": "rm -rf node_modules && yarn install && npm run clean && npm run build",
    "build": "node ./scripts/compile.js",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./",
    "check-and-publish": "node ./scripts/check-and-publish",
    "test": "build-scripts test",
    "clean": "rm -rf packages/*/node_modules && rm -rf ./packages/*/lib ./packages/*/es ./packages/*/dist"
  },
  "config": {
    "ghooks": {
      "commit-msg": "./scripts/validate-commit-msg.js"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": [
    "packages/*"
  ]
