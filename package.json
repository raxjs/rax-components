    "conventional-commit-types": "^2.2.0",
    "csstype": "^2.6.4",
    "cz-conventional-changelog": "^2.1.0",
    "driver-universal": "^1.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-rax": "^1.0.3",
    "eslint": "^6.0.0",
    "findup": "^0.1.5",
    "jest": "^26.0.1",
    "jest-diff": "^26.0.1",
    "jest-environment-node": "^26.0.1",
    "husky": "^4.2.5",
