<canvas class="{{className}}" 
    style="{{styleSheet}}"
    id="{{id}}"
    width="{{width}}"
    height="{{height}}"
    bindtap="onClick" 
    bindlongtap="onLongPress" 
    bindtouchstart="onTouchStart"
    bindtouchmove="onTouchMove"
    bindtouchend="onTouchEnd"
    bindtouchcancel="onTouchCancel">
</canvas>
