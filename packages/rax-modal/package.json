{
  "name": "rax-modal",
  "version": "1.2.0",
  "description": "rax-modal",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./package-lock.json",
    "build": "npm run clean && rax-scripts build",
    "start": "rax-scripts start",
    "prepublishOnly": "npm run build",
    "test": "rax-scripts test",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raxjs/rax-components.git"
  },
  "keywords": [
    "Rax",
    "rax-component"
  ],
  "engines": {
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "rax-text": "^1.1.0",
    "rax-view": "^1.1.0",
    "universal-env": "^3.0.0",
    "universal-transition": "^1.0.0"
  },
  "peerDependencies": {
    "rax": "^1.0.4"
  },
  "devDependencies": {
    "@rax-types/rax": "^1.0.6",
    "@types/jest": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "csstype": "^2.6.7",
    "driver-universal": "^1.0.2",
    "eslint": "^5.16.0",
    "eslint-config-rax": "^0.0.0",
    "husky": "^3.0.5",
    "lerna": "^3.16.4",
    "lint-staged": "^9.2.5",
    "rax": "^1.0.4",
    "rax-plugin-component": "^0.1.25",
    "rax-scripts": "^2.0.0",
    "rax-test-renderer": "^1.0.0",
    "typescript": "^3.6.2"
