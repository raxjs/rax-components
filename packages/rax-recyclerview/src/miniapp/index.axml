<rax-scrollview 
    className="{{className}}" 
    style="{{style}}" 
    endReachedThreshold="{{endReachedThreshold}}"
    onEndReached="onEndReached"
    onScroll="onScroll"
    onTouchStart="onTouchStart"
    onTouchMove="onTouchMove"
    onTouchEnd="onTouchEnd"
    onTouchCancel="onTouchCancel">
    <slot/>
</rax-scrollview>
