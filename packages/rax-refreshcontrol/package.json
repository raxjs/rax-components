{
  "name": "rax-refreshcontrol",
  "version": "1.0.0",
  "description": "RefreshControl component for Rax.",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./package-lock.json",
    "build": "npm run clean && rax-scripts build --type 'component'",
    "start": "rax-scripts start --type 'component' -p 8000",
    "miniapp": "rax-scripts start --type 'component-miniapp'",
    "prepublishOnly": "npm run build",
    "test": "npm run build && rax-scripts test",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./src --fix"
  },
  "files": [
    "dist",
    "lib"
  ],
  "miniappConfig": {
    "main": "lib/miniapp/index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raxjs/rax-refreshcontrol.git"
  },
  "keywords": [
    "Rax",
    "rax-component",
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/alibaba/rax/issues"
  },
  "homepage": "https://github.com/alibaba/rax#readme",
  "engines": {
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "universal-env": "^1.0.0"
  },
  "peerDependencies": {
    "rax": "^1.0.0"
  },
  "devDependencies": {
    "@rax-types/rax": "^1.0.5",
    "@types/ali-app": "^1.0.0",
    "@types/jest": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "csstype": "^2.6.4",
    "driver-dom": "^1.0.0",
    "driver-universal": "^1.0.1",
    "driver-weex": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-rax": "^0.0.0",
    "rax": "^1.0.0",
    "rax-recyclerview": "^1.0.0-beta.2",
    "rax-scripts": "1.2.1",
    "rax-test-renderer": "^1.0.0",
    "rax-text": "^1.0.1",
    "rax-view": "^1.0.1"
  }
}
