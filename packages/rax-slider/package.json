{
  "name": "rax-slider",
  "version": "1.1.1",
  "description": "Slider component for Rax.",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf ./lib && rm -rf ./package-lock.json && rm -f ./yarn.lock && rm -rf demo/miniapp/lib",
    "build": "npm run clean && rax-scripts build",
    "start": "rax-scripts start",
    "prepublishOnly": "npm run build",
    "test": "npm run build && rax-scripts test",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./src"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raxjs/rax-components.git"
  },
  "keywords": [
    "Rax",
    "rax-component",
    "react-component"
  ],
  "bugs": {
    "url": "https://github.com/alibaba/rax/issues"
  },
  "homepage": "https://github.com/raxjs/rax-slider#readme",
  "engines": {
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "rax-children": "^1.0.0",
    "rax-clone-element": "^1.0.0",
    "rax-find-dom-node": "^1.0.0",
    "rax-proptypes": "^1.0.0",
    "rax-view": "^1.1.0-0",
    "universal-env": "^2.0.0",
    "universal-panresponder": "^0.6.5"
  },
  "peerDependencies": {
    "rax": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "@rax-types/rax": "^1.0.5",
    "@types/ali-app": "^1.0.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "csstype": "^2.6.4",
    "driver-universal": "^1.0.0",
    "eslint": "^5.10.0",
    "eslint-config-rax": "^0.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "~7.11.1",
    "husky": "^3.0.5",
    "jest-localstorage-mock": "^2.3.0",
    "lint-staged": "^9.2.5",
    "rax": "^1.0.0",
    "rax-image": "^1.0.1-beta.0",
    "rax-plugin-component": "^0.1.9",
    "rax-scripts": "^2.0.0",
    "rax-test-renderer": "^1.0.0",
    "typescript": "^3.5.2"
  },
  "miniappConfig": {
    "main": "lib/miniapp/index",
    "subComponents": {
      "Item": {
        "tagNameMap": "swiper-item"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
